{
    "name": "Scratch Model Deep Dive v1",
    "description": "Initial experiments to explore different model configurations for Javanese ASR scratch model",
    "created_at": "2025-12-05",
    "experiments": [
        {
            "name": "S01: Baseline (Char, Pyr, LSTM)",
            "config": {
                "token_type": "char",
                "encoder_type": "pyramidal",
                "decoder_type": "lstm",
                "learning_rate": 5e-4,
                "num_epochs": 100
            }
        },
        {
            "name": "S02: Word Vocab (Word, Pyr, LSTM)",
            "config": {
                "token_type": "word",
                "encoder_type": "pyramidal",
                "decoder_type": "lstm",
                "learning_rate": 5e-4,
                "num_epochs": 100
            }
        },
        {
            "name": "S04: GRU Decoder (Char, Pyr, GRU)",
            "config": {
                "token_type": "char",
                "encoder_type": "pyramidal",
                "decoder_type": "gru",
                "learning_rate": 5e-4,
                "num_epochs": 100
            }
        },
        {
            "name": "S05: High LR (1e-3)",
            "config": {
                "token_type": "char",
                "encoder_type": "pyramidal",
                "decoder_type": "lstm",
                "learning_rate": 1e-3,
                "num_epochs": 100
            }
        },
        {
            "name": "S06: Low LR (1e-4)",
            "config": {
                "token_type": "char",
                "encoder_type": "pyramidal",
                "decoder_type": "lstm",
                "learning_rate": 1e-4,
                "num_epochs": 100
            }
        }
    ]
